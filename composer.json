{
  "name": "zetta-code/doctrine-util",
  "type": "library",
  "description": "Zetta\\DoctrineUtil",
  "license": "MIT",
  "keywords": [
    "zf3",
    "doctrine",
    "util"
  ],
  "homepage": "https://github.com/zetta-code/doctrine-util",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "homepage": "http://thiagossantos.com.br",
      "role": "Developer"
    }
